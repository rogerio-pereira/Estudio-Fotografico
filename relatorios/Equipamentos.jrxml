	<queryString>
		<![CDATA[SELECT
     empresa.`cnpj` AS empresa_cnpj,
     empresa.`Endereco` AS empresa_Endereco,
     empresa.`bairro` AS empresa_bairro,
     empresa.`cep` AS empresa_cep,
     empresa.`cidade` AS empresa_cidade,
     empresa.`complemento` AS empresa_complemento,
     empresa.`email` AS empresa_email,
     empresa.`estado` AS empresa_estado,
     empresa.`fax` AS empresa_fax,
     empresa.`nome` AS empresa_nome,
     empresa.`numero` AS empresa_numero,
     empresa.`site` AS empresa_site,
     empresa.`telefone` AS empresa_telefone,
     equipamentos.`codigo` AS equipamentos_codigo,
     equipamentos.`equipamento` AS equipamentos_equipamento,
     marcasequipamentos.`marca` AS marcasequipamentos_marca,
     equipamentos.`preco` AS equipamentos_preco,
     equipamentos.`dataCompra` AS equipamentos_dataCompra,
     equipamentos.`situacao` AS equipamentos_situacao,
     equipamentos.`obsSituacao` AS equipamentos_obsSituacao,
     tiposequipamentos.`tipo` AS tiposequipamentos_tipo
FROM
     `tiposequipamentos` tiposequipamentos INNER JOIN `equipamentos` equipamentos ON tiposequipamentos.`codigo` = equipamentos.`tipo_codigo`
     INNER JOIN `marcasequipamentos` marcasequipamentos ON equipamentos.`marca_codigo` = marcasequipamentos.`codigo`,
     `empresa` empresa
WHERE
     marcasequipamentos.`marca` LIKE '%$P!{MarcaEquipamento}%' AND
	tiposequipamentos.`tipo` LIKE '%$P!{TipoEquipamento}%' AND
	(
		equipamentos.`situacao` = $P{Situacao0} OR
		equipamentos.`situacao` = $P{Situacao1} OR
		equipamentos.`situacao` = $P{Situacao2} OR
		equipamentos.`situacao` = $P{Situacao3} OR
		equipamentos.`situacao` = $P{Situacao4} OR
		equipamentos.`situacao` = $P{Situacao5}
	)
ORDER BY $P{Order}]]>
	</queryString>
